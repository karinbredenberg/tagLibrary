         <eac-cpf:place xmlns:eac-cpf="urn:isbn:1-931666-33-4">
            <eac-cpf:placeRole>siege social</eac-cpf:placeRole>
            <eac-cpf:address>
               <eac-cpf:addressLine localType="voie">2 rue Corneille</eac-cpf:addressLine>
