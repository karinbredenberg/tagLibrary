<?xml version="1.0" encoding="UTF-8"?>
<div type="elementDocumentation" xml:id="outline">
   <head>
      <gi>outline</gi>
   </head>
   <div type="fullName">
      <p xml:lang="en">Outline</p>
   </div>
   <div type="summary">
      <p xml:lang="en">An element used within the description area to encode information in an
							outline format.</p>
   </div>
   <div type="description">
      <p xml:lang="en">
         <gi>outline</gi> contains one or more <gi>level</gi> elements, which contain either
									<gi>item</gi> elements or further <gi>level</gi> elements in a hierarchical
								fashion.</p>
   </div>
   <div type="mayContain">
      <p>level</p>
   </div>
   <div type="mayOccurWithin">
      <p>biogHist, functions, generalContext, legalStatuses, localDescriptions,
							mandates, occupations, places, structureOrGenealogy</p>
   </div>
   <div type="attributes">
      <p xml:lang="en">
         <list type="gloss">
            <label>localType</label>
            <item>Optional</item>
            <label>xml:id</label>
            <item>Optional</item>
            <label>xml:lang</label>
            <item>Optional</item>
         </list>
      </p>
   </div>
