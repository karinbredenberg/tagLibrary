            <item>Optional</item>
            <label>xml:id</label>
            <item>Optional</item>
            <label>xml:lang</label>
            <item>Optional</item>
         </list>
      </p>
   </div>
   <div type="reference">
      <p>ISAAR (CPF) </p>
   </div>
